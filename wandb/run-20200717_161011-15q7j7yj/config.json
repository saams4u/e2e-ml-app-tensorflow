{
  "cuda": false,
  "seed": 1234,
  "shuffle": true,
  "data_url": "https://raw.githubusercontent.com/madewithml/lessons/master/data/news.csv",
  "lower": true,
  "char_level": false,
  "filters": "[!\\\"'#$%&()*\\+,-./:;<=>?@\\\\\\[\\]^_`{|}~]",
  "data_size": 1.0,
  "train_size": 0.7,
  "val_size": 0.15,
  "test_size": 0.15,
  "num_epochs": 10,
  "batch_size": 64,
  "embedding_dim": 100,
  "use_glove": true,
  "freeze_embeddings": false,
  "filter_sizes": [
    2,
    3,
    4
  ],
  "num_filters": 50,
  "hidden_dim": 128,
  "dropout_p": 0.1,
  "learning_rate": 0.0001,
  "patience": 3
}